{
  "name": "@iliad.dev/ts-utils",
  "version": "0.0.19",
  "description": "A collection of TypeScript utilities.",
  "repository": {
    "type": "git",
    "url": "https://github.com/iliadwebdev/iliad-standard-issue"
  },
  "main": "./lib/index.cjs",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/index.cjs"
    },
    "./@types": {
      "types": "./@types/index.d.ts",
      "import": "./@types/index.js",
      "require": "./@types/index.cjs"
    }
  },
  "files": [
    "lib",
    "dist",
    "@types"
  ],
  "license": "ISC",
  "author": "Smoke3785",
  "scripts": {
    "ts-types": "tsc",
    "build:dev": "tsx scripts/generate-globals.ts && rimraf lib && tsup && tsx scripts/merge-globals.ts",
    "dev": "tsx scripts/generate-globals.ts && rimraf lib && tsup --watch",
    "build:prod": "tsx scripts/generate-globals.ts && rimraf dist && env NODE_ENV=production tsup && tsx scripts/merge-globals.ts",
    "merge-globals": "tsx scripts/merge-globals.ts",
    "build:watch": "tsup --watch"
  },
  "dependencies": {
    "@iliad.dev/build-scripts": "*",
    "@types/deasync": "^0.1.5",
    "deasync": "^0.1.30",
    "deepmerge": "^4.3.1",
    "deepmerge-ts": "^7.1.3",
    "install": "^0.13.0",
    "npm": "^10.9.1",
    "tslib": "~2.7"
  },
  "devDependencies": {
    "@eslint/js": "~9.10",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/eslint__js": "~8.42",
    "@types/node": "~20",
    "@types/qs": "^6.9.15",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "cpx": "^1.5.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "globals": "~15.9",
    "nodemon": "^3.1.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "rollup": "^4.17.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-api-utils": "~1.3",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.5",
    "typescript": "^5.2.2",
    "typescript-eslint": "~8.5"
  }
}
